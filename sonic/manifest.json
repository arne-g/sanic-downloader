{
  "name": "Sanic downloader",
  "version": "1.1",
  "manifest_version": 2,
  "description": "My first chrome extension",
  "permissions": ["tabs", "activeTab"],

  "browser_action": {
    "default_icon": "Sonic_Sprite.png",
    "default_popup": "popup.html"
  },

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  
  "content_scripts": [
    {
      "matches": ["https://kickass.unblocked.li/usearch/?q=*"],
      "js": ["kickass-content.js"]
    },
    {
      "matches": ["http://www.realitykings.com/*/*/home.htm"],
      "js": ["realitykings-content.js"]
    },
    {
      "matches": ["http://www.teamskeet.com/t1/trailer/view/*/*"],
      "js": ["teamskeet-content.js"]
    },
    {
      "matches": ["http://new.bangbros.com/videos/*.htm"],
      "js": ["bangbros-content.js"]
    },
    {
      "matches": ["http://tour.naughtyamerica.com/scenes/*/*/*"],
      "js": ["naughtyamerica-content.js"]
    },
    {
      "matches": ["http://www.brazzers.com/scenes/view/id/*/*/"],
      "js": ["brazzers-content.js"]
    },
    {
      "matches": ["http://www.digitalplayground.com/*/trailer/*/*/"],
      "js": ["digitalplayground-content.js"]
    },
    {
      "matches": ["http://ddfnetwork.com/preview/*/*.html"],
      "js": ["ddfnetwork-content.js"]
    },
    {
      "matches": ["http://pornprosnetwork.com/video/*"],
      "js": ["pornpros-content.js"]
    }
  ]
}
